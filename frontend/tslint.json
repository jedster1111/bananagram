
{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-console": {
      "severity": "warn"
    },
    "prettier": true,
    "interface-name": false,
    "no-empty-interface": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "semicolon": true
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}